<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">



    <bean id="foo" class="com.mouse.autumn.basicioc.factorybean.staticfactory01.Foo">
        <property name="barInterface">
            <ref bean="bar"></ref>
        </property>
    </bean>

    <bean id="bar" class="com.mouse.autumn.basicioc.factorybean.staticfactory01.StaticBarInterfaceFactory" factory-method="getInstance"></bean>



</beans>